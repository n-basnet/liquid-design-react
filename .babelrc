{
  "env": {
    "main": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ],
        "stage-0",
        "react"
      ],
      "plugins": [
        "add-react-displayname",
        [
          "babel-plugin-root-import",
          {
            "rootPathSuffix": "src"
          }
        ],
        "babel-plugin-styled-components",
        "transform-decorators-legacy",
        [
          "ramda",
          {
            "useES": true
          }
        ]
      ]
    },
    "test": {
      "presets": [["env"], "stage-0", "react"],
      "plugins": [
        [
          "babel-plugin-root-import",
          {
            "rootPathSuffix": "src"
          }
        ],
        "babel-plugin-styled-components",
        "transform-decorators-legacy"
      ]
    }
  }
}
