{
  "name": "liquid-design-react",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "cross-env CI=1 jest --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/*.js",
    "test:watch": "jest --watch",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cm": "git-cz",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "lint": "eslint ./src ./example/storybook",
    "add-component": "node scripts/addComponent.js"
  },
  "dependencies": {
    "polished": "^1.9.2",
    "styled-components": "^3.3.3",
    "styled-system": "^2.3.2",
    "system-components": "^2.2.2"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "camelcase": "^5.0.0",
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.1",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-extra": "^6.0.1",
    "husky": "^0.14.3",
    "inquirer": "^6.0.0",
    "jest-cli": "^23.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rollup": "^0.54.0",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^1.1.0",
    "rollup-plugin-react-svg": "^2.1.0",
    "rollup-plugin-url": "^1.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/src/empty-module.js"
    }
  },
  "files": [
    "dist"
  ]
}
