{
  "name": "@liquid-design/liquid-design-react",
  "version": "1.1.5-release.1",
  "description": "React UI component library for the Liquid Design System of Merck KGaA, Darmstadt, Germany",
  "author": "User Experience Strategy & Design unit, part of the Chief Digital Organization at Merck KGaA, Darmstadt, Germany",
  "license": "SEE LICENSE IN LICENSE.md",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "scripts": {
    "test": "cross-env CI=1 jest --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/*.js",
    "test:local": "jest",
    "test:watch": "jest --watch",
    "test:loki": "npm run storybook:build-loki && loki --requireReference --reactUri file:./out --verboseRenderer",
    "test:loki:approve": "loki approve",
    "test:loki:update": "npm run storybook:build-loki && loki update --requireReference --reactUri file:./out",
    "build": "npm run build:es && npm run build:cjs",
    "build:es": "cross-env BABEL_ENV=main rollup -c rollup.config.es.js",
    "build:cjs": "cross-env BABEL_ENV=main rollup -c rollup.config.cjs.js",
    "start": "cross-env BABEL_ENV=main rollup -c -w",
    "prepare": "yarn run build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cm": "git-cz",
    "lint": "eslint ./src ./storybook ./scripts",
    "format": "prettier --config .prettierrc --write './{src,storybook,scripts}/**/*{js,json}' && eslint ./src ./storybook ./scripts --ext js --quiet --fix",
    "add-component": "node scripts/addComponent.js",
    "prepare-svgs": "node scripts/prepareSVGs.js",
    "postinstall": "node scripts/remindOfTerms.js",
    "storybook:start": "cross-env BABEL_ENV=main start-storybook -s ./storybook/public -p 9001 -c storybook",
    "storybook:build": "cross-env BABEL_ENV=main build-storybook -s ./storybook/public -c storybook -o out",
    "storybook:build-loki": "STORYBOOK_LOKI_BUILD=1 npm run storybook:build",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "animate.css": "^3.6.1",
    "classnames": "^2.2.6",
    "date-fns": "^1.29.0",
    "javascript-natural-sort": "^0.7.1",
    "polished": "^3.0.0",
    "ramda": "^0.27.0",
    "rc-slider": "^8.6.1",
    "react-animated-transitions": "^0.0.6",
    "react-click-outside": "^3.0.1",
    "react-modal": "^3.5.1",
    "react-resizable": "^1.7.5",
    "react-resize-detector": "^3.1.2",
    "react-transition-group": "^2.4.0",
    "styled-components": "^3.3.3",
    "svg.js": "^2.6.6",
    "uniqid": "^5.0.3",
    "victory": "^32.3.7"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.2",
    "@storybook/addon-actions": "^4.1.18",
    "@storybook/addon-info": "^4.1.18",
    "@storybook/addon-knobs": "^3.4.12",
    "@storybook/addon-options": "^3.4.12",
    "@storybook/react": "^4.0.0-alpha.12",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-ramda": "^2.0.0",
    "camelcase": "^5.3.1",
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.23",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^4.0.1",
    "fast-lorem-ipsum": "^1.2.0",
    "final-form": "^4.18.4",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.3",
    "husky": "^2.7.0",
    "inquirer": "^6.5.1",
    "jest-cli": "^25.1.0",
    "lint-staged": "^8.2.1",
    "loki": "^0.17.0",
    "object-assign": "^4.1.1",
    "prettier": "^1.17.1",
    "prop-types": "^15.6.2",
    "raw-loader": "^0.5.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-final-form": "^3.7.0",
    "react-svg-loader": "^3.0.3",
    "rollup": "^1.30.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-react-svg": "^3.0.3",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-url": "^3.0.1",
    "s3-deploy": "^1.3.0",
    "semantic-release": "^17.0.2",
    "svgo": "^1.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "release",
        "prerelease": true
      }
    ],
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/gitlab"
    ],
    "prepare": [
      {
        "path": "@semantic-release/changelog",
        "changelogFile": "CHANGELOG.md"
      },
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/gitlab"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css|woff|woff2)$": "<rootDir>/src/moduleMocks/empty.js",
      "\\.(svg)$": "<rootDir>/src/moduleMocks/svg.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "loki": {
    "chromeSelector": "#storybook-main-element",
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1200,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false,
        "fitWindow": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:liquid-design/liquid-design-react.git"
  },
  "prettier": {
    "printWidth": 100
  },
  "sideEffects": false
}
